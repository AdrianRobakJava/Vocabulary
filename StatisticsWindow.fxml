<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.StatisticsWindow">
   <children>
      <VBox alignment="CENTER" prefHeight="500.0" prefWidth="700.0">
         <children>
            <TextArea fx:id="wordsListTA" editable="false" prefHeight="450.0" prefWidth="700.0" />
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="700.0">
               <children>
                  <Button fx:id="showB" mnemonicParsing="false" text="Show">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="closeB" mnemonicParsing="false" text="Close">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
